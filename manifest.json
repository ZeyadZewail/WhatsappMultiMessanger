{
    "name": "WhatsApp MultiMessanger",
    "description": "Send Messages to multiple people",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "WhatsApp.png"
    },
    "permissions": ["tabs","notifications","storage", "declarativeContent", "activeTab", "scripting"],
    "host_permissions": [
      "https://web.whatsapp.com/"
    ]
  }